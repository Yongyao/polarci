<UserControl x:Class="GeoSearch.RadioButtonWithTextAndMultiIcon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300" d:DesignWidth="400">
    <StackPanel Name="StackPanelArea1" Orientation="Horizontal" Background="White">
        <RadioButton Name="isSelected_RadioButton" VerticalAlignment="Center" IsChecked="False"/>
        <Image Name="Image_Map" Source="/GeoSearch;component/images/resourceTypes/Layers-icon.png" Height="30" Width="30" Stretch="Fill" Margin="1,0,0,0" VerticalAlignment="Center"/>
        <Image Name="Image_Layer" Source="/GeoSearch;component/images/resourceTypes/map.png" Height="30" Width="30" Stretch="Fill" Margin="1,0,0,0" VerticalAlignment="Center"/>
        <Image Name="Image_QoS" Source="/GeoSearch;component/images/QoS/5bars.png" Height="20" Width="20" Stretch="Fill" Margin="1,0,0,0" VerticalAlignment="Center"/>
        <TextBlock Name="TextBlock_Name"  Text="xxxxxxx" Foreground="Black" FontStyle="Normal" VerticalAlignment="Center" Margin="3,0,0,0" FontSize="14"/>
    </StackPanel>
</UserControl>
